// html style tags

body
    font-family "Helvetica Neue", "Lucida Grande", "Arial"    
    padding 0px
    height 100%
    background-image url(../images/bg1.gif)

a:link, a:visited, a:hover
    color white
    text-decoration underline

a:hover
    text-decoration none

h4
    font-size 13px
    font-weight normal
    color #333333

 

// Header and Main Layout

#container
    width 800px
    margin 0 auto

#bartop
    width 800px
    height 28px
    background-color #333333
    margin 0
    float left

#barcolour
    width 800px
    height 9px
    background-color #00498f
    margin 0
    float left

#barspacer
    width 800px
    height 2px
    background-color #FFFFFF
    margin 0
    float left

#masthead1
    width 250px
    height 140px
